{
  "name": "touchswap-telegram-bot",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@grammyjs/auto-chat-action": "^0.1.1",
    "@grammyjs/fluent": "^1.0.3",
    "@grammyjs/hydrate": "^1.4.1", 
    "@grammyjs/menu": "^1.2.1",
    "@grammyjs/parse-mode": "^1.9.0",
    "@grammyjs/router": "^2.0.0",
    "@moebius/fluent": "^1.1.0",
    "@types/node": "^20.12.7",
    "callback-data": "^1.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "grammy": "^1.22.4",
    "grammy-guard": "^0.5.0",
    "iso-639-1": "^3.1.2",
    "node-graceful-shutdown": "^1.1.5",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.7.3",
    "znv": "^0.4.0",
    "zod": "^3.23.4"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "tsc-watch --onSuccess \"tsx ./main.ts\"",
    "start": "tsc && tsx ./main.ts",
    "start:force": "tsx ./src/main.ts",
    "check-types": "tsc --noEmit --incremental",
    "format": "prettier --write . '!(node_modules)/**/*'",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "prepare": "yarn husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~5.40.0",
    "autoprefixer": "~10.4.12",
    "eslint": "~8.24.0",
    "eslint-config-next": "~13.1.6",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2",
    "prettier": "~2.8.4",
    "tsc-watch": "^6.2.0",
    "type-fest": "~4.6.0",
    "typescript": "~5.1.6"
  },
  "lint-staged": {
    "*.ts": "yarn lint"
  }
}
